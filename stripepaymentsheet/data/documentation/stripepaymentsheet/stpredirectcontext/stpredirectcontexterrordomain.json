textErrorDomain":{"identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/STPRedirectContextErrorDomain","kind":"symbol","abstract":[{"text":"The domain for NSErrors specific to ","type":"text"},{"type":"codeVoice","code":"STPRedirectContext"}],"role":"symbol","title":"STPRedirectContextErrorDomain","fragments":[{"kind":"keyword","text":"static"},{"text":" ","kind":"text"},{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContextErrorDomain"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:SS","text":"String"}],"url":"\/documentation\/stripepaymentsheet\/stpredirectcontext\/stpredirectcontexterrordomain","type":"topic"},"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet":{"identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet","role":"collection","abstract":[{"text":"PaymentSheet is a prebuilt UI that combines all the steps required to pay - collecting payment details, billing details, and confirming the payment - into a single sheet that displays on top of your app.","type":"text"}],"url":"\/documentation\/stripepaymentsheet","kind":"symbol","title":"StripePaymentSheet","type":"topic"},"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet/STPRedirectContext":{"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"title":"STPRedirectContext","url":"\/documentation\/stripepaymentsheet\/stpredirectcontext","type":"topic","role":"symbol","identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"text":"STPRedirectContext","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"type":"text","text":" "},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"type":"text","text":" "},{"text":"when the redirect completes.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"text":" ","type":"text"},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"type":"text","text":" "},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"text":" ","type":"text"},{"type":"text","text":"detect when the user has completed the necessary redirect action by listening"},{"text":" ","type":"text"},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"type":"text","text":" "},{"type":"text","text":"You should not use either this class, nor "},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"type":"text","text":" "},{"text":"state changes and to make the charge.","type":"text"}]}}}
