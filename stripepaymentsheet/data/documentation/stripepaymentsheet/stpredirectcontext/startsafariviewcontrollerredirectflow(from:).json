modules":[{"name":"StripePaymentSheet"}],"symbolKind":"method","role":"symbol","roleHeading":"Instance Method","title":"startSafariViewControllerRedirectFlow(from:)","externalID":"c:@M@StripePayments@objc(cs)STPRedirectContext(im)startSafariViewControllerRedirectFlowFromViewController:","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"startSafariViewControllerRedirectFlow","kind":"identifier"},{"kind":"text","text":"("},{"text":"from","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIViewController","text":"UIViewController"},{"kind":"text","text":")"}]},"abstract":[{"text":"Starts a redirect flow by presenting an SFSafariViewController in your app","type":"text"},{"type":"text","text":" "},{"type":"text","text":"from the passed in view controller."},{"text":" ","type":"text"},{"type":"text","text":"You must ensure that your app delegate listens for  the "},{"type":"codeVoice","code":"returnURL"},{"type":"text","text":" that you"},{"text":" ","type":"text"},{"type":"text","text":"set on the Stripe object, and forwards it to the Stripe SDK so that the"},{"text":" ","type":"text"},{"text":"context can be notified when the redirect is completed and dismiss the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"view controller. See "},{"type":"codeVoice","code":"StripeAPI.handleStripeURLCallback(with url:)]"},{"text":" ","type":"text"},{"type":"text","text":"The context will listen for both received URLs and app open notifications"},{"text":" ","type":"text"},{"text":"and fire its completion block when either the URL is received, or the next","type":"text"},{"type":"text","text":" "},{"type":"text","text":"time the app is foregrounded."}],"identifier":{"url":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/startSafariViewControllerRedirectFlow(from:)","interfaceLanguage":"swift"},"references":{"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet/STPRedirectContext/startSafariViewControllerRedirectFlow(from:)":{"kind":"symbol","role":"symbol","type":"topic","title":"startSafariViewControllerRedirectFlow(from:)","identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/startSafariViewControllerRedirectFlow(from:)","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"startSafariViewControllerRedirectFlow","kind":"identifier"},{"text":"(","kind":"text"},{"text":"from","kind":"externalParam"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIViewController","text":"UIViewController"},{"text":")","kind":"text"}],"url":"\/documentation\/stripepaymentsheet\/stpredirectcontext\/startsafariviewcontrollerredirectflow(from:)","abstract":[{"type":"text","text":"Starts a redirect flow by presenting an SFSafariViewController in your app"},{"text":" ","type":"text"},{"text":"from the passed in view controller.","type":"text"},{"text":" ","type":"text"},{"text":"You must ensure that your app delegate listens for  the ","type":"text"},{"code":"returnURL","type":"codeVoice"},{"text":" that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"set on the Stripe object, and forwards it to the Stripe SDK so that the"},{"type":"text","text":" "},{"text":"context can be notified when the redirect is completed and dismiss the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"view controller. See "},{"type":"codeVoice","code":"StripeAPI.handleStripeURLCallback(with url:)]"},{"text":" ","type":"text"},{"type":"text","text":"The context will listen for both received URLs and app open notifications"},{"type":"text","text":" "},{"type":"text","text":"and fire its completion block when either the URL is received, or the next"},{"text":" ","type":"text"},{"text":"time the app is foregrounded.","type":"text"}]},"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet":{"identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet","role":"collection","abstract":[{"text":"PaymentSheet is a prebuilt UI that combines all the steps required to pay - collecting payment details, billing details, and confirming the payment - into a single sheet that displays on top of your app.","type":"text"}],"url":"\/documentation\/stripepaymentsheet","kind":"symbol","title":"StripePaymentSheet","type":"topic"},"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet/STPRedirectContext":{"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"title":"STPRedirectContext","url":"\/documentation\/stripepaymentsheet\/stpredirectcontext","type":"topic","role":"symbol","identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"text":"STPRedirectContext","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"type":"text","text":" "},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"type":"text","text":" "},{"text":"when the redirect completes.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"text":" ","type":"text"},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"type":"text","text":" "},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"text":" ","type":"text"},{"type":"text","text":"detect when the user has completed the necessary redirect action by listening"},{"text":" ","type":"text"},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"type":"text","text":" "},{"type":"text","text":"You should not use either this class, nor "},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"type":"text","text":" "},{"text":"state changes and to make the charge.","type":"text"}]}}}
